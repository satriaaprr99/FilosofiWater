<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="narsis.xiirpl2.filosofiwater.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_feed"
        android:name="narsis.xiirpl2.filosofiwater.ui.feed.FeedFragment"
        android:label="@string/title_feed"
        tools:layout="@layout/fragment_feed" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="narsis.xiirpl2.filosofiwater.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="narsis.xiirpl2.filosofiwater.ui.profile.ProfileFragment"
        android:label="@string/title_profile" />
    <fragment
        android:id="@+id/navigation_shop"
        android:name="narsis.xiirpl2.filosofiwater.ui.shop.ShopFragment"
        android:label="@string/title_shop" />
</navigation>